                    <StaticResource x:Key="ToggleButtonForegroundChecked"
                                    ResourceKey="AccentContrastColorBrush" />
                    <StaticResource x:Key="ToggleButtonForegroundCheckedPointerOver"
                                    ResourceKey="AccentContrastColorBrush" />
                    <StaticResource x:Key="ToggleButtonForegroundCheckedPressed"
                                    ResourceKey="ActiveTextColorBrush" />

                    <!--#endregion-->

                    <!--#region CheckBox Control-->

                    <StaticResource x:Key="CheckBoxForegroundUnchecked"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxForegroundUncheckedPointerOver"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxForegroundUncheckedPressed"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="CheckBoxForegroundChecked"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxForegroundCheckedPointerOver"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxForegroundCheckedPressed"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="CheckBoxForegroundIndeterminate"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxForegroundIndeterminatePointerOver"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxForegroundIndeterminatePressed"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="CheckBoxCheckGlyphForegroundChecked"
                                    ResourceKey="AccentContrastColorBrush" />
                    <StaticResource x:Key="CheckBoxCheckGlyphForegroundCheckedPointerOver"
                                    ResourceKey="AccentContrastColorBrush" />
                    <StaticResource x:Key="CheckBoxCheckGlyphForegroundCheckedPressed"
                                    ResourceKey="ActiveTextColorBrush" />

                    <StaticResource x:Key="CheckBoxCheckGlyphForegroundIndeterminate"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxCheckGlyphForegroundIndeterminatePointerOver"
                                    ResourceKey="ActiveTextColorBrush" />
                    <StaticResource x:Key="CheckBoxCheckGlyphForegroundIndeterminatePressed"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="CheckBoxCheckBackgroundStrokeUnchecked"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="CheckBoxCheckBackgroundStrokeUncheckedPointerOver"
                                    ResourceKey="ActiveTextColorBrush" />

                    <StaticResource x:Key="CheckBoxCheckBackgroundStrokeCheckedPointerOver"
                                    ResourceKey="ActiveTextColorBrush" />

                    <StaticResource x:Key="CheckBoxCheckBackgroundFillChecked"
                                    ResourceKey="AccentColorBrush" />
                    <StaticResource x:Key="CheckBoxCheckBackgroundFillCheckedPointerOver"
                                    ResourceKey="AccentColorBrush" />

                    <StaticResource x:Key="CheckBoxCheckBackgroundStrokeIndeterminate"
                                    ResourceKey="AccentColorBrush" />
                    <StaticResource x:Key="CheckBoxCheckBackgroundStrokeIndeterminatePointerOver"
                                    ResourceKey="AccentColorBrush" />

                    <!--#endregion-->

                    <!--#region RadioButton Control-->

                    <StaticResource x:Key="RadioButtonForeground"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="RadioButtonForegroundPointerOver"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="RadioButtonForegroundPressed"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="RadioButtonOuterEllipseStroke"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="RadioButtonOuterEllipseStrokePointerOver"
                                    ResourceKey="ActiveTextColorBrush" />
                    <StaticResource x:Key="RadioButtonOuterEllipseStrokePressed"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="RadioButtonOuterEllipseCheckedStroke"
                                    ResourceKey="AccentColorBrush" />
                    <StaticResource x:Key="RadioButtonOuterEllipseCheckedStrokePointerOver"
                                    ResourceKey="AccentColorBrush" />
                    <StaticResource x:Key="RadioButtonOuterEllipseCheckedStrokePressed"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="RadioButtonCheckGlyphFill"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="RadioButtonCheckGlyphFillPointerOver"
                                    ResourceKey="ActiveTextColorBrush" />
                    <StaticResource x:Key="RadioButtonCheckGlyphFillPressed"
                                    ResourceKey="TextColorBrush" />

                    <!--#endregion-->

                    <!--#region ToggleSwitch Control-->

                    <StaticResource x:Key="ToggleSwitchHeaderForeground"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="ToggleSwitchContentForeground"
                                    ResourceKey="TextColorBrush" />

                    <StaticResource x:Key="ToggleSwitchStrokeOff"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="ToggleSwitchStrokeOffPointerOver"
                                    ResourceKey="ActiveTextColorBrush" />

                    <StaticResource x:Key="ToggleSwitchKnobFillOff"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="ToggleSwitchKnobFillOffPointerOver"
                                    ResourceKey="ActiveTextColorBrush" />
                    <StaticResource x:Key="ToggleSwitchKnobFillOffPressed"
                                    ResourceKey="AccentContrastColorBrush" />

                    <StaticResource x:Key="ToggleSwitchKnobFillOn"
                                    ResourceKey="AccentContrastColorBrush" />
                    <StaticResource x:Key="ToggleSwitchKnobFillOnPointerOver"
                                    ResourceKey="ActiveAccentContrastColorBrush" />
                    <StaticResource x:Key="ToggleSwitchKnobFillOnPressed"
                                    ResourceKey="AccentContrastColorBrush" />

                    <StaticResource x:Key="ToggleSwitchFillOn"
                                    ResourceKey="AccentColorBrush" />
                    <StaticResource x:Key="ToggleSwitchFillOnPointerOver"
                                    ResourceKey="ActiveAccentColorBrush" />

                    <!--#endregion-->

                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
