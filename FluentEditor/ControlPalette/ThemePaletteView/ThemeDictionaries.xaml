<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>

                    <!--#region Controls Colors-->

                    <SolidColorBrush x:Key="ControlForeground"
                                     Color="{Binding DarkTheme.ControlForegroundColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ControlDisableForeground"
                                     Color="{Binding DarkTheme.ControlDisableForegroundColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ControlActiveForeground"
                                     Color="{Binding DarkTheme.ControlActiveForegroundColor, Source={StaticResource ThemeManager}}" />

                    <SolidColorBrush x:Key="ControlBackground"
                                     Color="{Binding DarkTheme.ControlBackgroundColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ControlDisableBackground"
                                     Color="{Binding DarkTheme.ControlDisableBackgroundColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ControlActiveBackground"
                                     Color="{Binding DarkTheme.ControlActiveBackgroundColor, Source={StaticResource ThemeManager}}" />

                    <!--#endregion-->
                    
                    <!--#region Background colors-->

                    <SolidColorBrush x:Key="AppBackground"
                                     Color="{Binding DarkTheme.AppBackground, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ItemBackground0"
                                     Color="{Binding DarkTheme.ItemBackground0, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ItemBackground1"
                                     Color="{Binding DarkTheme.ItemBackground1, Source={StaticResource ThemeManager}}" />
                    <AcrylicBrush x:Key="AppAcrylicBrush"
                                  BackgroundSource="Backdrop"
                                  TintOpacity="{Binding DarkTheme.AcrylicBackgroundOpacity, Source={StaticResource ThemeManager}}"
                                  TintColor="{Binding DarkTheme.AcrylicBackgroundColor, Source={StaticResource ThemeManager}}"
                                  FallbackColor="{Binding DarkTheme.AcrylicBackgroundColor, Source={StaticResource ThemeManager}}" />

                    <!--#endregion-->

                    <!--#region TextBlock colors-->

                    <SolidColorBrush x:Key="TextColorBrush"
                                     Color="{Binding DarkTheme.TextColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="SubTextColorBrush"
                                     Color="{Binding DarkTheme.SubTextColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="SubSubTextColorBrush"
                                     Color="{Binding DarkTheme.SubSubTextColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ActiveTextColorBrush"
                                     Color="{Binding DarkTheme.ActiveTextColor, Source={StaticResource ThemeManager}}" />
                    <!--#endregion-->

                    <!--#region Accent colors-->

                    <SolidColorBrush x:Key="AccentColorBrush"
                                     Color="{Binding DarkTheme.AccentColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ActiveAccentColorBrush"
                                     Color="{Binding DarkTheme.ActiveAccentColor, Source={StaticResource ThemeManager}}" />

                    <SolidColorBrush x:Key="AccentContrastColorBrush"
                                     Color="{Binding DarkTheme.AccentContrastColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="ActiveAccentContrastColorBrush"
                                     Color="{Binding DarkTheme.ActiveAccentContrastColor, Source={StaticResource ThemeManager}}" />

                    <!--#endregion-->

                    <!--#region HyperlinkButton Control-->

                    <SolidColorBrush x:Key="HyperlinkButtonForeground"
                                     Color="{Binding DarkTheme.HyperlinkButtonForegroundColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="HyperlinkButtonForegroundPressed"
                                     Color="{Binding DarkTheme.HyperlinkButtonActiveForegroundColor, Source={StaticResource ThemeManager}}" />
                    <SolidColorBrush x:Key="HyperlinkButtonForegroundPointerOver"
                                     Color="{Binding DarkTheme.HyperlinkButtonActiveForegroundColor, Source={StaticResource ThemeManager}}" />

                    <!--#endregion-->

                    <!--#region Button Control-->

                    <StaticResource x:Key="ButtonForeground"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="ButtonForegroundPointerOver"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="ButtonForegroundPressed"
                                    ResourceKey="ActiveTextColorBrush" />

                    <!--#endregion-->

                    <!--#region TextBlockButton Control-->

                    <StaticResource x:Key="TextBlockButtonForeground"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="TextBlockButtonForegroundPressed"
                                    ResourceKey="ActiveTextColorBrush" />
                    <StaticResource x:Key="TextBlockButtonForegroundPointerOver"
                                    ResourceKey="ActiveTextColorBrush" />

                    <!--#endregion-->

                    <!--#region AccentButton Control-->

                    <StaticResource x:Key="AccentButtonForeground"
                                    ResourceKey="AccentContrastColorBrush" />
                    <StaticResource x:Key="AccentButtonForegroundPointerOver"
                                    ResourceKey="AccentContrastColorBrush" />
                    <StaticResource x:Key="AccentButtonForegroundPressed"
                                    ResourceKey="ActiveTextColorBrush" />

                    <StaticResource x:Key="AccentButtonBackground"
                                    ResourceKey="AccentColorBrush" />
                    <StaticResource x:Key="AccentButtonBackgroundPointerOver"
                                    ResourceKey="AccentColorBrush" />

                    <!--#endregion-->

                    <!--#region ToggleButton Control-->

                    <StaticResource x:Key="ToggleButtonBackgroundChecked"
                                    ResourceKey="AccentColorBrush" />
                    <StaticResource x:Key="ToggleButtonBackgroundCheckedPointerOver"
                                    ResourceKey="AccentColorBrush" />

                    <StaticResource x:Key="ToggleButtonForeground"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="ToggleButtonForegroundPointerOver"
                                    ResourceKey="TextColorBrush" />
                    <StaticResource x:Key="ToggleButtonForegroundPressed"
